stage("parallel") {
    parallel (
        'thread 1': {
            sleep 1
            echo "thread 1 - 1"
            sleep 2
            echo "thread 1 - 2"
        },
        'thread 2': {
            sleep 2
            echo "thread 2 - 1"
            sleep 1
            echo "thread 2 - 2"
        }
    )
}