stage("parallel_stages") {
    parallel
        first: stage("parallel_stage1") {
            sleep 1
            echo "11111111111"
            sleep 2
            echo "11111111111"
        },
        second: stage("parallel_stage2") {
            sleep 2
            echo "22222222222"
            sleep 1
            echo "22222222222"
        },
        failFast: false
}